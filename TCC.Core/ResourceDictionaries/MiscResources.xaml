<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Storyboard x:Key="Pulse" RepeatBehavior="Forever" Duration="0:0:2" Timeline.DesiredFrameRate="20">
        <DoubleAnimation From="1" To="1.1" Duration="0:0:0.2" EasingFunction="{StaticResource QuadraticEase}" Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleX)" />
        <DoubleAnimation From="1" To="1.1" Duration="0:0:0.2" EasingFunction="{StaticResource QuadraticEase}" Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleY)" />
        <DoubleAnimation From="1.1" To="1" Duration="0:0:0.2" EasingFunction="{StaticResource QuadraticEase}" Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleX)" BeginTime="0:0:0.2"/>
        <DoubleAnimation From="1.1" To="1" Duration="0:0:0.2" EasingFunction="{StaticResource QuadraticEase}" Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleY)" BeginTime="0:0:0.2"/>
    </Storyboard>

    <Storyboard x:Key="Warn" RepeatBehavior="Forever">
        <DoubleAnimation Storyboard.TargetProperty="Opacity" Duration="0:0:0.2" From="0" To="1"  EasingFunction="{StaticResource QuadraticEase}" />
        <DoubleAnimation Storyboard.TargetProperty="Opacity" Duration="0:0:0.6" From="1" To="0"  EasingFunction="{StaticResource QuadraticEase}" BeginTime="0:0:0.2" />
    </Storyboard>

    <!-- Fonts -->
    <FontFamily x:Key="Frutiger">                  pack://application:,,,/resources/fonts/#Frutiger LT Pro 55 Roman              </FontFamily>
    <FontFamily x:Key="ArialMonoBold">             pack://application:,,,/resources/fonts/#Arial Monospaced MT                   </FontFamily>
    <FontFamily x:Key="NotoSansMed">               pack://application:,,,/resources/fonts/#Noto Sans Med                         </FontFamily>

    <!-- Embedded images -->
    <ImageSource x:Key="DefaultGuildLogo">         pack://application:,,,/resources/images/default_guild_logo.png                </ImageSource>
    <ImageSource x:Key="MoongourdLogo">            pack://application:,,,/resources/images/moongourd.png                         </ImageSource>

    <ImageSource x:Key="BlankImage">               pack://application:,,,/resources/images/Icon_Laurels/blank.png                </ImageSource>
    <ImageSource x:Key="BronzeLaurel">             pack://application:,,,/resources/images/Icon_Laurels/bronze.png               </ImageSource>
    <ImageSource x:Key="BronzeLaurelNew">          pack://application:,,,/resources/images/Icon_Laurels/bronze_kr.png            </ImageSource>
    <ImageSource x:Key="BronzeLaurelNewBig">       pack://application:,,,/resources/images/Icon_Laurels/bronze_kr_big.png        </ImageSource>
    <ImageSource x:Key="BronzeLaurelNewBottom">    pack://application:,,,/resources/images/Icon_Laurels/bronze_kr_bottom.png     </ImageSource>
    <ImageSource x:Key="SilverLaurel">             pack://application:,,,/resources/images/Icon_Laurels/silver.png               </ImageSource>
    <ImageSource x:Key="SilverLaurelNew">          pack://application:,,,/resources/images/Icon_Laurels/silver_kr.png            </ImageSource>
    <ImageSource x:Key="SilverLaurelNewBig">       pack://application:,,,/resources/images/Icon_Laurels/silver_kr_big.png        </ImageSource>
    <ImageSource x:Key="SilverLaurelNewBottom">    pack://application:,,,/resources/images/Icon_Laurels/silver_kr_bottom.png     </ImageSource>
    <ImageSource x:Key="GoldLaurel">               pack://application:,,,/resources/images/Icon_Laurels/gold.png                 </ImageSource>
    <ImageSource x:Key="GoldLaurelNew">            pack://application:,,,/resources/images/Icon_Laurels/gold_kr.png              </ImageSource>
    <ImageSource x:Key="GoldLaurelNewBig">         pack://application:,,,/resources/images/Icon_Laurels/gold_kr_big.png          </ImageSource>
    <ImageSource x:Key="GoldLaurelNewBottom">      pack://application:,,,/resources/images/Icon_Laurels/gold_kr_bottom.png       </ImageSource>
    <ImageSource x:Key="DiamondLaurel">            pack://application:,,,/resources/images/Icon_Laurels/diamond.png              </ImageSource>
    <ImageSource x:Key="DiamondLaurelNew">         pack://application:,,,/resources/images/Icon_Laurels/diamond_kr.png           </ImageSource>
    <ImageSource x:Key="DiamondLaurelNewBig">      pack://application:,,,/resources/images/Icon_Laurels/diamond_kr_big.png       </ImageSource>
    <ImageSource x:Key="DiamondLaurelNewBottom">   pack://application:,,,/resources/images/Icon_Laurels/diamond_kr_bottom.png    </ImageSource>
    <ImageSource x:Key="ChampionLaurel">           pack://application:,,,/resources/images/Icon_Laurels/champion.png             </ImageSource>
    <ImageSource x:Key="ChampionLaurelNew">        pack://application:,,,/resources/images/Icon_Laurels/champion_kr.png          </ImageSource>
    <ImageSource x:Key="ChampionLaurelNewBig">     pack://application:,,,/resources/images/Icon_Laurels/champion_kr_big.png      </ImageSource>
    <ImageSource x:Key="ChampionLaurelNewBottom">  pack://application:,,,/resources/images/Icon_Laurels/champion_kr_bottom.png   </ImageSource>

    <DropShadowEffect x:Key="ClassIconGlow"                ShadowDepth="0" BlurRadius="3"  Opacity=".5"  Color="{StaticResource ClassGlowColor}" />
    <DropShadowEffect x:Key="ClassWindowSkillBorderShadow" ShadowDepth="0" BlurRadius="20" />

</ResourceDictionary>