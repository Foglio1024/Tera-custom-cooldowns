                <TextBlock Foreground="#90ffffff" FontWeight="Bold">NO</TextBlock>

            </Button>
            <UniformGrid.LayoutTransform>
                <ScaleTransform ScaleY="0"/>
            </UniformGrid.LayoutTransform>
        </UniformGrid>
    </Grid>
